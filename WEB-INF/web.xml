<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <servlet>
    <servlet-name>DADDBConnection</servlet-name>
    <servlet-class>Mimap.DADDBConnection</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DADUser</servlet-name>
    <servlet-class>Mimap.DADUser</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DADUserForm</servlet-name>
    <servlet-class>Mimap.DADUserForm</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DADSendMail</servlet-name>
    <servlet-class>Mimap.DADSendMail</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DADDBConnection</servlet-name>
    <url-pattern>/servlet/Mimap.DADDBConnection</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DADUser</servlet-name>
    <url-pattern>/servlet/Mimap.DADUser</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DADUserForm</servlet-name>
    <url-pattern>/servlet/Mimap.DADUserForm</url-pattern>
  </servlet-mapping>

  <session-config>
    <session-timeout>
            200
        </session-timeout>
  </session-config>
  <welcome-file-list>
    <welcome-file>
            index.jsp
        </welcome-file>
    <welcome-file>
            index.html
        </welcome-file>
    <welcome-file>
            index.htm
        </welcome-file>
  </welcome-file-list>
</web-app>
